extends layout
 
block title
	title Tax Bracket Example
 
block scripts
	script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")
	script(src="/index.js")
block content
	h1 Paycheck Calculator
	p Tax Brackets:
	
	p Federal 2015 Single Tax Bracket:
	
	each taxBracket in database
		p Jurisdiction: #{taxBracket.name} - Min AGI: #{taxBracket.minagi} - Max AGI: #{taxBracket.maxagi}
	
	input.addTxtBox(type='text')
