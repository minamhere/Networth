extends layout
 
block title
	title Networth Example
 
block scripts
	script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")
	script(src="/index.js")

block content
	h1 Paycheck Calculator
	p People in the database:
	

	p Users:
	
	!{JSON.stringify(pageInfo.users)}
	
	each user in pageInfo.users
			p Name: #{user.full_name} - Email Address: #{user.email_address} - Birthdate: #{user.birthdate}
	
	p Brackets:
	
	!{JSON.stringify(pageInfo.taxBrackets)}
	
