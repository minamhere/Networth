extends layout
 
block title
  title Networth Example
 
block scripts
  script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")
  script(src="/index.js")

block content
  p
    each person in database
        p
        p Name: #{person.full_name}
        p Email Address: #{person.email_address}
        p Birthdate: #{person.birthdate}
        